{
  "name": "voice-agent",
  "version": "1.0.0",
  "type": "module",
  "description": "Voice & SMS Agent with GPT-5 integration for Outcome Workspace",
  "main": "server-modular.js",
  "scripts": {
    "start": "node server-modular.js",
    "start:monitor": "./start-with-logs.sh",
    "dev": "node --inspect server-modular.js",
    "test": "node test-cli-simple.js",
    "logs": "tail -f logs/latest.log",
    "logs:error": "tail -f logs/latest.log | grep -E 'error|Error|ERROR|❌|failed|Failed'",
    "build": "echo 'Build complete - Node.js server ready for deployment'"
  },
  "keywords": [
    "voice-agent",
    "sms",
    "gpt-5",
    "fastify",
    "twilio",
    "openai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/formbody": "^8.0.2",
    "@fastify/websocket": "^11.2.0",
    "@openai/agents": "^0.1.0",
    "@openai/agents-extensions": "^0.1.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.5.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.20.1",
    "twilio": "^5.9.0",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
